{
  "name": "upload-release-action",
  "version": "4.1.0",
  "private": true,
  "description": "Upload files to a GitHub release",
  "main": "lib/main.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "dist": "ncc build",
    "test": "jest",
    "all": "yarn format && yarn lint && yarn build && yarn dist && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svestaro/upload-release-action.git"
  },
  "keywords": [
    "actions",
    "upload",
    "release",
    "assets",
    "asset"
  ],
  "author": "Termux Maintainers",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@types/glob": "^8.1.0",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.2",
    "@typescript-eslint/parser": "^7.12.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
